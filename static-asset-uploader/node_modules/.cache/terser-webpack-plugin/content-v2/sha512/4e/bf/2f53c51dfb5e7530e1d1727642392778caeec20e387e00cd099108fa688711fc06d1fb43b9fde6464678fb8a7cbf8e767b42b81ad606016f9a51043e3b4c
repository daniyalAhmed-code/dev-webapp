TED MODULE: ./src/utils/use-boolean.jsx\n/**\n * A React state hook wrapping a boolean value, returning `setTrue` and\n * `setFalse` functions which do as their names suggest. Returns `[state,\n * setTrue, setFalse, setState]`, in which the first and last functions\n * correspond to those returned by `useState`, and in which all functions (i.e.\n * all but `state`) are stable.\n *\n * This is especially useful for controlled modals, for example, which may\n * close themselves via a callback:\n * ```javascript\n * const [isOpen, open, close] = useBoolean(false)\n * return (<Modal isOpen={isOpen} closeSelf={close}> ... </Modal>)\n * ```\n */const useBoolean=initialState=>{const[state,setState]=Object(react[\"useState\"])(initialState);const setTrue=Object(react[\"useCallback\"])(()=>setState(true),[]);const setFalse=Object(react[\"useCallback\"])(()=>setState(false),[]);return[state,setTrue,setFalse,setState];};\n// CONCATENATED MODULE: ./src/pages/Admin/Accounts/PendingInvites.jsx\nconst PendingInvites=()=>{const[accounts,setAccounts]=Object(react[\"useState\"])([]);const[loading,setLoading]=Object(react[\"useState\"])(true);const[selectedAccount,setSelectedAccount]=Object(react[\"useState\"])(undefined);const[isCreateModalOpen,openCreateModal,closeCreateModal]=useBoolean(false);const[isDeleteModalOpen,openDeleteModal,closeDeleteModal]=useBoolean(false);const[messages,sendMessage]=useMessages();const[createModalMessages,sendCreateModalMessage,clearCreateModalMessages]=useMessages();const refreshAccounts=()=>fetchPendingInviteAccounts().then(accounts=>setAccounts(accounts));// Initial load\nObject(react[\"useEffect\"])(()=>{refreshAccounts().finally(()=>setLoading(false));},[]);const onSelectAccount=Object(react[\"useCallback\"])(account=>setSelectedAccount(account),[]);const onConfirmCreate=Object(react[\"useCallback\"])(async emailAddress=>{setLoading(true);clearCreateModalMessages();try{await createInviteByEmail(emailAddress);}catch(error){sendCreateModalMessage(di